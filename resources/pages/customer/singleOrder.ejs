<main class="singleOrder">
    <section class="container md:w-2/3 px-2 md:mx-auto py-8" >
        <div class="flex justify-between items-center">
            <h2 class="font-bold py-4"> Track Delivery Status  </h2>
            <small class="bg-white rounded p-2 text-green-700"> <%= order._id %> </small>
        </div>
        <div class="singleOrder_status mt-4">
            <input id="getOrder" type="hidden" value="<%= JSON.stringify(order) %>" >
            <ul>
                <li data-status="order_placed" class="order_status pb-16"> <span> Order Placed </span> </li>
                <li data-status="processing" class="order_status pb-16"> <span> Preparing </span> </li>
                <li data-status="delivering" class="order_status pb-16"> <span> Out for Delivery </span> </li>
                <li data-status="completed" class="order_status pb-16"> <span> Completed </span> </li>
            </ul>
        </div>
    </section>
</main>